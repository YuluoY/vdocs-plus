p > 0) {\r\n            ratio = 1\r\n            r = -r\r\n            d = -d\r\n        }\r\n        node.timer = setInterval(() => {\r\n            if (Math.abs(gap) >= destination || destination <= 0) {\r\n                clearInterval(node.timer)\r\n                node.timer = undefined\r\n                setTimeout(() => this.exec(node, 50, 2, 10), 2000)\r\n            }\r\n            node.style.opacity = ratio += r\r\n            if (gap < 0) {\r\n                node.style.transform = `translateX(${gap += n}px)`\r\n            } else {\r\n                node.style.transform = `translateX(${d += n}px)`\r\n            }\r\n        }, speed)\r\n        return this\r\n    }\r\n}\r\n\r\nmodule.exports = codes => {\r\n    for (let i = 0; i < codes.length; i++) {\r\n        codes[i].parentNode.appendChild(copyNode(codes[i], i))\r\n    }\r\n    window.$hUtils.clipboard = function (index) {\r\n        navigator.clipboard.writeText(codes[index].innerText).then(value => {\r\n            const tips = $.getElementsByClassName(`copyTips-${index}`)[0];\r\n            tips.style.display = 'inline'\r\n            toggle.exec(tips, 40, -2, 10, 2000)\r\n        })\r\n    }\r\n}"],"mappings":";;;;AAAA;AACA;AACA;AACA;AACA;AACA,eAAiBA,OAAO,CAAC,SAAD,CAAxB;AAAA,IAAOC,MAAP,YAAOA,MAAP;;AACA,IAAMC,CAAC,GAAGC,QAAV;;AAGA,SAASC,QAAT,CAAkBC,IAAlB,EAAwBC,CAAxB,EAA2B;EACvB,IAAMC,WAAW,GAAGL,CAAC,CAACM,aAAF,CAAgB,KAAhB,CAApB;EACAD,WAAW,CAACE,SAAZ,GAAwB,cAAxB;EACAF,WAAW,CAACG,SAAZ,uDACqCJ,CADrC,gIAEoEA,CAFpE;EAGA,OAAOC,WAAP;AACH;;AAED,IAAMI,MAAM,GAAG;EACXC,IAAI,EAAE,cAAUC,IAAV,EAAgBC,WAAhB,EAA6BC,GAA7B,EAA8C;IAAA;;IAAA,IAAZC,KAAY,uEAAJ,EAAI;IAChDC,aAAa,CAACJ,IAAI,CAACK,KAAN,CAAb;IACA,IAAIC,CAAC,GAAGJ,GAAR;IACA,IAAIK,KAAK,GAAG,KAAKN,WAAW,GAAGO,IAAI,CAACC,GAAL,CAASP,GAAT,CAAnB,CAAZ;IACA,IAAIQ,CAAC,GAAGH,KAAR;IACA,IAAII,CAAC,GAAGV,WAAR;;IACA,IAAIC,GAAG,GAAG,CAAV,EAAa;MACTK,KAAK,GAAG,CAAR;MACAG,CAAC,GAAG,CAACA,CAAL;MACAC,CAAC,GAAG,CAACA,CAAL;IACH;;IACDX,IAAI,CAACK,KAAL,GAAaO,WAAW,CAAC,YAAM;MAC3